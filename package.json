    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "webflow:deploy": "webflow cloud deploy",
    "version:alpha": "node scripts/increment-alpha.js",
    "deploy:alpha": "npm run version:alpha && git add package.json && git commit -m \"ðŸš€ Alpha $(node -p \"require('./package.json').alphaVersion\") release\" && git push && npm run webflow:deploy"
